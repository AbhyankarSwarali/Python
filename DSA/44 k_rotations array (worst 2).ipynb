   "source": [
    "l = [3,9,5,6,7,8]\n",
    "k = 3\n",
    "def k_rot(l): \n",
    "    n = len(l)\n",
    "    rotate = k % n\n",
    "    for i in range(rotate): \n",
    "        e = l.pop()\n",
    "        l.insert(0,e)\n",
    "    print(l)\n",
    "\n",
    "k_rot(l)"
   ]
