    "n = [5,7,8,4,1,6,9,2]\n",
    "def asc_sort(n, idx): \n",
    "    for i in range(len(n)-1): \n",
    "        small = n[i]\n",
    "        if n[i+1] < n[i]: \n",
    "            small = n[i+1]\n",
    "            n[i], n[i+1] = n[i+1], n[i]\n",
    "            asc_sort(n, idx+1)\n",
    "    return n\n",
    "\n",
    "asc_sort(n,0)"
   ]
