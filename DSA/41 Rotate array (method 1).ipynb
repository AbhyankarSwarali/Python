   "source": [
    "nums = [5,-2,3,9,0,6,10]\n",
    "def rotate(nums): \n",
    "    n = len(nums)\n",
    "    if n == 1:\n",
    "        return nums\n",
    "    nums[:] = nums[-1:] + nums[:-1]\n",
    "    return nums\n",
    "\n",
    "print(rotate(nums))"
   ]
  }
