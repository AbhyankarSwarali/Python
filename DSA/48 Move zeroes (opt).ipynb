   "source": [
    "l = [1,0,2,4,3,0,0,3,5,1]\n",
    "if len(l) == 1: \n",
    "    print(l)\n",
    "i = 0\n",
    "while i < len(l):\n",
    "    if l[i] == 0: \n",
    "        break\n",
    "    i += 1\n",
    "if i == len(l): \n",
    "    print(l)\n",
    "j = i+1\n",
    "while j < len(l): \n",
    "    if l[j] != 0: \n",
    "        l[i], l[j] = l[j], l[i]\n",
    "        i += 1\n",
    "    j += 1\n",
    "print(l)"
   ]
  }
