   "source": [
    "l = [5,7,3,2,6,1,5,9]\n",
    "def reverse(l, left, right): \n",
    "    if left >= right: \n",
    "        return\n",
    "    l[left], l[right] = l[right], l[left]\n",
    "    reverse(l, left+1, right-1)\n",
    "\n",
    "reverse(l,0,7)\n",
    "print(l)"
   ]
  }
