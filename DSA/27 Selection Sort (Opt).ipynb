   "source": [
    "l = [5,7,8,4,1,6,9,2]\n",
    "def asc_sort(l): \n",
    "    n = len(l)\n",
    "    for i in range(n): \n",
    "        idx = i\n",
    "        for j in range(i+1, n):\n",
    "            if l[j] < l[idx]: \n",
    "                idx = j\n",
    "        l[i], l[idx] = l[idx], l[i]\n",
    "    return l\n",
    "\n",
    "asc_sort(l)"
   ]
  }
