   "source": [
    "def Largest(n): \n",
    "    largest = float('-inf')\n",
    "    l = len(n)\n",
    "    for i in range(l): \n",
    "        largest = max(largest, n[i])\n",
    "    return largest\n",
    "\n",
    "n = [55,32,-97,99,3,67]\n",
    "print(Largest(n))"
   ]
  }
