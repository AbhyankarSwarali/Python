   "source": [
    "l = [5,10,-3,-1,-10,6]\n",
    "n = len(l)\n",
    "pos = []\n",
    "neg = []\n",
    "for i in range(n): \n",
    "    if l[i] > 0: \n",
    "        pos.append(l[i])\n",
    "    else: \n",
    "        neg.append(l[i])\n",
    "for j in range(len(pos)): \n",
    "    l[2*j] = pos[j]\n",
    "    l[(2*j) + 1] = neg[j]\n",
    "print(l)"
   ]
  }
