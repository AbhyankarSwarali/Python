    "def rec_pali(s,left,right): \n",
    "    if left >= right: \n",
    "        return\n",
    "    if s[left] == s[right]: \n",
    "        rec_pali(s, left+1, right-1)\n",
    "        return True\n",
    "    else: \n",
    "        return False\n",
    "\n",
    "s = 'python'\n",
    "res = rec_pali(s, 0, len(s)-1)\n",
    "print(res)"
   ]
  }
