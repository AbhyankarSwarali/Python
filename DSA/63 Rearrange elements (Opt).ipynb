   "source": [
    "l = [5,10,-3,-1,-10,6]\n",
    "n = len(l)\n",
    "res = [0] * n\n",
    "pos,neg = 0,1\n",
    "for i in range(n):\n",
    "    if l[i] >= 0:\n",
    "        res[pos] = l[i]\n",
    "        pos += 2\n",
    "    else: \n",
    "        res[neg] = l[i]\n",
    "        neg += 2\n",
    "print(res)"
   ]
  }
