   "source": [
    "price = [7,2,1,5,6,4,8]\n",
    "profit = 0\n",
    "n = len(price)\n",
    "for i in range(n): \n",
    "    for j in range(i+1,n): \n",
    "        if price[j] > price[i]: \n",
    "            p = price[j] - price[i]\n",
    "            profit = max(p,profit)\n",
    "print(profit)"
   ]
