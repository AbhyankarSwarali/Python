    "price = [7,2,1,5,6,4,8]\n",
    "n = len(price)\n",
    "profit = 0\n",
    "min_price = float('inf')\n",
    "for i in range(n): \n",
    "    min_price = min(min_price,price[i])\n",
    "    profit = max(profit,price[i] - min_price)\n",
    "print(profit)"
