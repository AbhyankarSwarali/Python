    "s = 'nitin'\n",
    "n = len(s)\n",
    "right = n - 1\n",
    "left = 0\n",
    "while left < right:\n",
    "    if s[left] != s[right]:\n",
    "        print(False)\n",
    "        break\n",
    "    left += 1\n",
    "    right -= 1\n",
    "else:\n",
    "    print(True)\n"
