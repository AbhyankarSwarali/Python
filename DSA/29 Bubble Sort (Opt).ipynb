   "source": [
    "l = [1,11,2,5,12,10,15,18] \n",
    "n = len(l)\n",
    "for i in range(n-2,-1,-1): \n",
    "    is_swap = False\n",
    "    for j in range(i+1): \n",
    "        if l[j] > l[j+1]: \n",
    "            l[j], l[j+1] = l[j+1], l[j]\n",
    "            is_swap = True\n",
    "if is_swap == False:\n",
    "    print(l)"
   ]
  }
