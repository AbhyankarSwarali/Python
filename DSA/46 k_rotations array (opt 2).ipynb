   "source": [
    "l = [3,9,5,6,7,8]\n",
    "k = 3\n",
    "n = len(l)\n",
    "def k_rot(l,left,right): \n",
    "    while left < right: \n",
    "        l[left], l[right] = l[right], l[left]\n",
    "        left += 1\n",
    "        right -= 1\n",
    "    return l\n",
    "\n",
    "k_rot(l, n-k, n-1)\n",
    "k_rot(l, 0, n-k-1)\n",
    "print(k_rot(l, 0, n-1))"
   ]
  }
 ],
