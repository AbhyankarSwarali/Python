    "def split_and_join(line):\n",
    "    spl = line.split()\n",
    "    res = '-'.join(spl)\n",
    "    return res\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    line = input()\n",
    "    result = split_and_join(line)\n",
    "    print(result)"
