   "source": [
    "num = int(input(\"Enter a number:\"))\n",
    "x = num\n",
    "result = 0\n",
    "while x > 0: \n",
    "    ld = x % 10\n",
    "    result = result*10 + ld\n",
    "    x = x // 10\n",
    "\n",
    "print(\"Original number:\", num)\n",
    "print('Reversed number:',result)"
   ]
  }
