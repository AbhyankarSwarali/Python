    "import re\n",
    "s = input()\n",
    "k = input()\n",
    "pattern = re.compile(k)\n",
    "r = pattern.search(s)\n",
    "if not r:\n",
    "    print('(-1, -1)')\n",
    "\n",
    "else:\n",
    "    while r: \n",
    "        print('({0}, {1})'.format(r.start(), r.end() - 1))\n",
    "        r = pattern.search(s, r.start() + 1)"
   ]
  }
