    "from itertools import combinations\n",
    "n = int(input())\n",
    "alphabets = input().split()\n",
    "k = int(input())\n",
    "count = 0\n",
    "comb_alpha = list(combinations(alphabets,k))\n",
    "for c in comb_alpha: \n",
    "    if 'a' in c:\n",
    "        count += 1\n",
    "print((count / len(comb_alpha)))"
   ]
  }
