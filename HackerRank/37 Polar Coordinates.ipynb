    "import cmath\n",
    "def polar_coords(z): \n",
    "    r = abs(z)\n",
    "    theta = cmath.phase(z)\n",
    "    return r,theta\n",
    "\n",
    "z = complex(input().strip())\n",
    "r, theta = polar_coords(z)\n",
    "\n",
    "print(round(r, 3))\n",
    "print(round(theta, 3))"
   ]
  }
